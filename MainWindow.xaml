                                <local:BarChart_NumberOfClaims/>
                            </oxy:PlotView.DataContext>
                        </oxy:PlotView>

                        <oxy:PlotView x:Name="Claimed_Value" Model="{Binding Model}" Grid.ColumnSpan="1" Grid.Column="1" Margin="0" >
                            <oxy:PlotView.DataContext>
                                <local:PieChart_ClaimedValue/>
                            </oxy:PlotView.DataContext>
                        </oxy:PlotView>

                        <oxy:PlotView x:Name="LabourClaimed"  Model="{Binding Model}" Grid.ColumnSpan="1" Grid.Column="2" Margin="0">
                            <oxy:PlotView.DataContext>
                                <local:BarChart_LabourClaimed/>
                            </oxy:PlotView.DataContext>
                        </oxy:PlotView>
                        <ComboBox x:Name="cbxMakeT1" Grid.Column="3" HorizontalAlignment="Left" Margin="143,95,0,0" VerticalAlignment="Top" Width="155"/>
                        <Label x:Name="lblMakeT1" Content="Make:" Grid.Column="3" HorizontalAlignment="Left" Margin="51,88,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold"/>
                        <Label x:Name="lblStatusT1" Content="Status:" Grid.Column="3" HorizontalAlignment="Left" Margin="51,122,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold"/>
                        <ComboBox x:Name="cbxStatusT1" Grid.Column="3" HorizontalAlignment="Left" Margin="143,129,0,0" VerticalAlignment="Top" Width="155"/>
                        <Label x:Name="lblType" Content="Claim type:" Grid.Column="3" HorizontalAlignment="Left" Margin="51,156,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold"/>
                        <ComboBox x:Name="cbxClaimTypeT1" Grid.Column="3" HorizontalAlignment="Left" Margin="143,163,0,0" VerticalAlignment="Top" Width="155"/>
