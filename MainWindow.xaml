                            <ColumnDefinition Width="1*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Label x:Name="lblStartDateT1" Content="Start date:" HorizontalAlignment="Left" Margin="51,22,0,0" VerticalAlignment="Top" FontSize="13.333" FontWeight="Bold" Grid.Column="3"/>
